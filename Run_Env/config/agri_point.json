{
  "LocalDomain": {
    "type": "point",
    "start_date": "2000/01/01",
    "end_date": "2005/12/31",
    "sequencer_library": "internal.flint",
    "sequencer": "CalendarAndEventFlintDataSequencer",
    "simulateLandUnit": "simulateLandUnit",
    "landUnitBuildSuccess": "landUnitBuildSuccess",
    "operationManager": {
      "name": "Simple",
      "use_kahan": false,
      "allow_zero_result_transfers": false
    }
  },
  "Pools": [
    "initial_values",
    "atmosphere",
    "soil",
    "debris"
  ],
  "Variables": [
    {
      "localDomainId": 1
    },
    {
      "simulateLandUnit": true
    },
    {
      "landUnitBuildSuccess": true
    },
    {
      "landUnitArea": 1.0
    },
    {
      "EF_1": {
        "default": 0.01,
        "dry": 0.005,
        "synth_wet": 0.016,
        "wet": 0.006
      }
    },
    {
      "EF_3": {
        "cattle_default": 0.004,
        "cattle_wet": 0.006,
        "cattle_dry": 0.002,
        "other": 0.003
      }
    },
    {
      "ipcc_climate_zone": "Tropical Dry"
    },
    {
      "soil_type": "HAC"
    },
    {
      "landtype": [ "F", "C", "C", "G", "G", "G"]
    },
    {
      "landuse": [ "", "Long term cultivated", "Perennial crop", "", "", ""]
    },
    {
      "change": {
        "transform": {
          "allow_nulls": true,
          "library": "moja.flint.example.agri",
          "type": "CompositeTimeSeriesTransform",
          "data_property": "data_yearly",
          "vars": [ "landtype", "landuse", "management", "input"]
        }
      }
    },
    {
      "management": [ "", "Reduced", "Full", "High intensity grazing", "High intensity grazing", "High intensity grazing"]
    },
    {
      "input": [ "", "Medium", "Medium", "High", "High", "High"]
    },
    {
      "region": "Latin America"
    },
    {
      "spatialLocationInfo": {
        "flintdata": {
          "library": "internal.flint",
          "type": "SpatialLocationInfo",
          "settings": {}
        }
      }
    },
    {
      "FCR_table": {
        "transform": {
          "library": "internal.flint",
          "type": "SQLQueryTransform",
          "provider": "SQLite",
          "queryFile": "config/sql/FCR_table.sql"
        }
      }
    },
    {
      "Cf_table": {
        "transform": {
          "library": "internal.flint",
          "type": "SQLQueryTransform",
          "provider": "SQLite",
          "queryFile": "config/sql/Cf_table.sql"
        }
      }
    },
    {
      "Animal_weights": {
        "transform": {
          "library": "internal.flint",
          "type": "SQLQueryTransform",
          "provider": "SQLite",
          "queryFile": "config/sql/Animal_weights.sql"
        }
      }
    },
    {
      "N_Excretion_rate": {
        "transform": {
          "library": "internal.flint",
          "type": "SQLQueryTransform",
          "provider": "SQLite",
          "queryFile": "config/sql/N_Excretion_rate.sql"
        }
      }
    },
    {
      "AWMS": {
        "transform": {
          "library": "internal.flint",
          "type": "SQLQueryTransform",
          "provider": "SQLite",
          "queryFile": "config/sql/AWMS.sql"
        }
      }
    },
    {
      "Soil_Stock_Change": {
        "transform": {
          "library": "internal.flint",
          "type": "SQLQueryTransform",
          "provider": "SQLite",
          "queryFile": "config/sql/Soil_Stock_Change.sql"
        }
      }
    },
    {
      "SOC_REF": {
        "transform": {
          "library": "internal.flint",
          "type": "SQLQueryTransform",
          "provider": "SQLite",
          "queryFile": "config/sql/SOC_REF.sql"
        }
      }
    },
    {
      "Wet_Dry_Climate": {
        "transform": {
          "library": "internal.flint",
          "type": "SQLQueryTransform",
          "provider": "SQLite",
          "queryFile": "config/sql/Wet_Dry_Climate.sql"
        }
      }
    },
    {
      "animal_type" : "default"
    },
    {
      "crop_type": "default"
    },
    {
      "yield": 0
    },
    {
      "harvested": 0
    },
    {
      "eventqueue": {
        "flintdata": {
          "library": "internal.flint",
          "type": "EventQueue",
          "settings": {
            "events": [
              {
                "date": {
                  "$date": "2000/01/01"
                },
                "id": 1,
                "type": "agri.NFertEvent",
                "name": "Synthetic fertilizer",
                "quantity": 100,
                "runtime": 5
              },
              {
                "date": {
                  "$date": "2001/05/01"
                },
                "id": 2,
                "type": "agri.NFertEvent",
                "name": "Organic fertilizer",
                "quantity": 200,
                "runtime": 5
              },
              {
                "date": {
                  "$date": "2002/03/01"
                },
                "id": 3,
                "type": "agri.PlantEvent",
                "name": "Winter Wheat"
              },
              {
                "date": {
                  "$date": "2002/07/01"
                },
                "id": 3,
                "type": "agri.HarvestEvent",
                "name": "Winter Wheat",
                "frac_burnt": 0,
                "frac_renew": 0,
                "frac_remove": 0
              },
              {
                "date": {
                  "$date": "2003/08/01"
                },
                "id": 3,
                "type": "agri.PRPEvent",
                "name": "PRP Event",
                "animal_type": "Buffalo",
                "no_livestock": 3,
                "productivity_class": "high",
                "use": "Other"
              }
            ]
          }
        }
      }
    }
  ],
  "Modules": {
    "TransactionManagerEndOfStepModule": {
      "library": "internal.flint",
      "order": 1
    },
    "CalendarAndEventFlintDataSequencer": {
      "library": "internal.flint",
      "order": 2
    },
    "DisturbanceEventModule": {
      "enabled": true,
      "library": "moja.flint.example.agri",
      "order": 3
    },
    "LandUseModule": {
      "enabled": true,
      "library": "moja.flint.example.agri",
      "order": 4
    },
    "OutputerStream": {
      "enabled": true,
      "library": "internal.flint",
      "order": 5,
      "settings": {
        "output_filename": "Example_Agri_Point_Stock.csv",
        "output_to_screen": false,
        "output_info_header": true,
        "output_on_postdisturbanceevent": true
      }
    },
    "OutputerStreamFlux": {
      "enabled": true,
      "library": "internal.flint",
      "order": 6,
      "settings": {
        "output_filename": "Example_Agri_Point_Flux.csv",
        "output_to_screen": true,
        "output_info_header": true
      }
    }
  }
}